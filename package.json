{
  "name": "@muchi-ui/muchi-ui",
  "version": "1.0.2",
  "type": "module",
  "description": "Muchi UI Components",
  "main": "./dist/muchi-ui.js",
  "styles": "./dist/style.css",
  "module": "./dist/muchi-ui.umd.js",
  "author": "MissWhale <doden5180@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MissWhale/muchi-ui"
  },
  "files": [
    "dist/",
    "src/components"
  ],
  "exports": {
    ".": {
      "import": "./dist/muchi-ui.js",
      "require": "./dist/muchi-ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install",
    "release": "standard-version"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.1.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@iconify/vue": "^4.1.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.25",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "path": "^0.12.7",
    "prettier": "^3.0.3",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.71.1",
    "standard-version": "^9.5.0",
    "typescript": "~5.4.0",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vite-plugin-vue-devtools": "^7.0.16",
    "vitest": "^1.3.1",
    "vue-tsc": "^2.0.6"
  }
}
